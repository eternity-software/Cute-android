<resources>
    <declare-styleable name="LocalizableTextView">
        <attr name="localizableKey" format="string" />
        <attr name="themeColor" format="string" />
        <attr name="themeColorLink" format="string" />
    </declare-styleable>
    <declare-styleable name="LocalizableButton">
        <attr name="localizableButtonKey" format="string" />
    </declare-styleable>
    <declare-styleable name="LocalizableEditText">
        <attr name="localizableHint" format="string" />
    </declare-styleable>
    <declare-styleable name="ErrorPanel">
        <attr name="text" format="string" />
        <attr name="title" format="string" />
        <attr name="localizableTextKey" format="string" />
        <attr name="localizableTitleKey" format="string" />
    </declare-styleable>
    <declare-styleable name="LightToolbar">
        <attr name="caption" format="string" />
        <attr name="captionLocId" format="string" />
    </declare-styleable>
    <declare-styleable name="ChangeOutlineRadius">
        <attr name="startRadius" format="dimension" />
        <attr name="endRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ThemeImageView">
        <attr name="themeImageColor" format="string" />
    </declare-styleable>
    <declare-styleable name="ThemeLinearLayout">
        <attr name="themeBackgroundColor" format="string" />
    </declare-styleable>
